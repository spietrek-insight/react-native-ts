{
  "name": "react-native-ts",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "expo:install": "npx expo install --fix",
    "start": "expo start",
    "ios:dev": "expo start --ios",
    "web:dev": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rimraf .expo android coverage ios",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "e2e:prepare": "expo prebuild",
    "e2e:ios:build": "detox build --configuration ios.release",
    "e2e:ios:test": "detox test --configuration ios.release",
    "e2e:ios": "run-s e2e:ios:*",
    "prepare": "simple-git-hooks",
    "e2e:playwright": "playwright test",
    "storybook:web": "storybook dev -p 6006",
    "storybook": "EXPO_PUBLIC_STORYBOOK=1 expo start -c",
    "storybook:ios": "EXPO_PUBLIC_STORYBOOK=1 expo start --ios -c",
    "storybook:android": "EXPO_PUBLIC_STORYBOOK=1 expo start --android -c",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "dupe:check": "jscpd --pattern \"src/**/*.ts*\" --ignore \"**/*.spec.ts\""
  },
